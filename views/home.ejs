<%- include("partials/header"); -%>

<h1>Home</h1>

<p><%= startingContent %></p>

<% posts.forEach(post => {%>
<h1><%=post.postTitle %></h1>
<p> <%= post.postContent.substring(0, 100) + "..."%> <a href="/posts/<%= post._id %> ">Read More</a> </p>
<%}); %>

<form action="/compose" method="get">
<button class="btn btn-primary" style="float: right;">Create New</button>
</form>

<%- include("partials/footer"); -%>